<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
  <package name="vdc_bkmanage" extends="default">
    <action name="getBKListBeforApprove" class="backUpManageAction" method="getBKListBeforApprove">
      <result type="json" name="success">
        <param name="ignoreHierarchy">false</param>
        <param name="root">listResp</param>
      </result>
      <result type="json" name="error">
        <param name="root">message</param>
      </result>
    </action>    
  
    <action name="vdcbkTemplateList" class="backUpManageAction" method="templateList">
      <result type="json" name="success">
        <param name="ignoreHierarchy">false</param>
        <param name="root">templateList</param>
      </result>
      <result type="json" name="error">
        <param name="root">message</param>
      </result>
    </action>
    <action name="applyChange" class="backUpManageAction" method="applyChange">
      <result type="json" name="success">
        <param name="ignoreHierarchy">false</param>
        <param name="root">message</param>
      </result>
      <result type="json" name="error">
        <param name="root">message</param>
      </result>
    </action>
     
    <action name="backupDestroy" class="backUpManageAction" method="backupDestroy">
      <result type="json" name="success">
        <param name="ignoreHierarchy">false</param>
        <param name="root">message</param>
      </result>
      <result type="json" name="error">
        <param name="root">message</param>
      </result>
    </action>  
    
    <action name="checkbackupDestroy" class="backUpManageAction" method="checkbackupDestroy">
      <result type="json" name="success">
        <param name="ignoreHierarchy">false</param>
        <param name="root">message</param>
      </result>
      <result type="json" name="error">
        <param name="root">message</param>
      </result>
    </action>       
    
    <action name="vdcbkProductList" class="backUpManageAction" method="productList">
      <result type="json" name="success">
        <param name="ignoreHierarchy">false</param>
        <param name="root">proList</param>
      </result>
      <result type="json" name="error">
        <param name="root">message</param>
      </result>
    </action>
     
  </package>
</struts>